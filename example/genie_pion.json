{
  "files": [
    "/media/storage/neutrino/GENIE/simple_flux_check/atmo/*/*.gtrac.root"
  ],
  "treename": "gRooTracker",
  "plugins": [
    {
      "name": "plot_pion",
      "plugin": "libgenie_pion.so",
      "preprocess": "CC_selection",
      "normalize": {
        "name": "normalize_factor_CC",
        "config": {
          "filename": "/media/storage/neutrino/GENIE/atmo/G18_10b_02_11b.root",
          "obj_path":"nu_mu_C12/tot_cc",
          "Z": 12
        }
      },
      "analysis": [
        {
          "treename": "treeout",
          "filename": "pion_kin.root",
          "plot_file": "plot_pion.root",
          "func": {
            "name": "pion_kin"
          },
          "output": [
            "pionE",
            "piontheta"
          ],
          "plots": [
            {
              "var": "pionE",
              "name": "pionE",
              "xmin": 0,
              "xmax": 1.0,
              "bins": 25
            },
            {
              "var": "piontheta",
              "name": "piontheta",
              "xmin": 0,
              "xmax": 1.0,
              "bins": 25
            }
          ]
        }
      ]
    }
  ]
}