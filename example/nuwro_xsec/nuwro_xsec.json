{
  "files": [
    "root://junoeos01.ihep.ac.cn//eos/juno/dirac/juno/user/q/qyan/nuwro/nuwro${flavor}-${model}-*/*.rootracker.root"
  ],
  "treename": "nRooTracker",
  "plugins": [
    {
      "name": "plot",
      "plugin": "${basepath}/build/example/nuwro_xsec/libnuwro_xsec.so",
      "normalize": {
        "name": "normalize_factor_nuwro",
        "config": {
          "bin_count": ${bins:256},
          "Emin": 0,
          "Emax": ${emax:20}
        }
      },
      "analysis": [
        {
          "treename": "treeout",
          "filename": "xsecplot1.root",
          "plot_file": "nuwro${flavor}-${model}.root",
          "func": "xsecplot",
          "output": [],
          "plots": [
            {
              "var": "E",
              "name": "xsec",
              "xmin": 0,
              "xmax": ${emax:20},
              "bins": ${bins:256}
            }
          ]
        }
      ]
    }
  ]
}